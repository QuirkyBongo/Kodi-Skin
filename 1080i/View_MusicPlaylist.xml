<?xml version="1.0" encoding="UTF-8"?>
<includes>
    <!--playlist-->
    <include name="musicPlayList">
        <control type="group">
            <visible>$EXP[isMusicPlaylist]</visible>
            <control type="panel" id="59">
                <posx>col-1-begin</posx>
                <posy>row-5-begin</posy>
